{
  "name": "mig-ui-plugin",
  "version": "0.0.0",
  "private": true,
  "repository": "git@github.com:rawagner/mig-ui-plugin.git",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "yarn clean && NODE_ENV=production yarn ts-node ./node_modules/.bin/webpack",
    "build-dev": "yarn clean && yarn ts-node ./node_modules/.bin/webpack",
    "http-server": "./http-server.sh ./dist",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}' -I '/node_modules/(?!(@console)/)/'"
  },
  "dependencies": {
    "@konveyor/lib-ui": "^5.1.7",
    "@patternfly/react-core": "^4.157.3",
    "@patternfly/react-tokens": "^4.12.18",
    "@types/node": "^14.14.34",
    "axios": "^0.21.4",
    "css-loader": "^5.0.1",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "http-proxy-middleware": "^2.0.1",
    "https-proxy-agent": "^5.0.0",
    "react": "16.9.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.11",
    "react-query": "^3.24.4",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "simple-oauth2": "^4.2.0",
    "style-loader": "^3.3.0",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@openshift-console/dynamic-plugin-sdk": "0.0.0-beta1",
    "@types/react": "16.8.13",
    "@types/react-router-dom": "5.1.2",
    "comment-json": "4.x",
    "http-server": "0.12.x",
    "read-pkg": "5.x",
    "ts-loader": "6.2.2",
    "ts-node": "5.0.1",
    "typescript": "3.8.3",
    "webpack": "5.0.0-beta.16",
    "webpack-cli": "4.5.x"
  },
  "consolePlugin": {
    "name": "mig-ui-plugin",
    "version": "0.0.1",
    "displayName": "Mig UI Plugin",
    "description": "Plugin for MTC within the console.",
    "exposedModules": {
      "barUtils": "./utils/bar",
      "clustersComp": "./components/ClustersPage.tsx",
      "plansComp": "./components/PlansPage.tsx"
    },
    "dependencies": {
      "@console/pluginAPI": "*"
    }
  }
}
